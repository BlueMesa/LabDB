{
    "name": "bluemesa/labdb",
    "description": "Symfony2-based LIMS for Drosophila Labs",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.6.0",
        "symfony/symfony": "~2.7.0",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "twig/extensions": "~1.3",
        "jms/security-extra-bundle": "~1.5.1",
        "jms/di-extra-bundle": "~1.6.0",
        "jms/serializer-bundle": "~1.0.0",
        "mopa/bootstrap-bundle": "~3.0.0-rc2",
        "twbs/bootstrap": "~3.3.0",
        "knplabs/knp-paginator-bundle": "~2.5.0",
        "knplabs/knp-menu-bundle": "~2.1.0",
        "craue/formflow-bundle": "~2.1.8",
        "friendsofsymfony/user-bundle": "~1.3.6",
        "friendsofsymfony/rest-bundle": "~1.7.2",
        "whiteoctober/tcpdf-bundle": "~1.0.1",
        "sabre/vobject": "~3.4.7",
        "liuggio/ExcelBundle": "~2.0",
        "rejsmont/php-ipp": "~0.21",
        "kuleuven/shibboleth-bundle": "dev-master#67070be",
        "oyejorge/less.php": "~1.7",
        "leafo/scssphp": "~0.3",
        "twitter/typeahead.js": "~0.11.1",
        "twitter/hogan.js": "v3.0.2",
        "fortawesome/font-awesome": "~4.4",
        "eternicode/bootstrap-datepicker": "~1.5.0",
        "ivaynberg/select2": "~3.5.4",
        "jquery/jquery": "~1.11",
        "jquery/jquery-ui": "~1.11",
        "packagist/yuicompressor-bin": "~2.4.8",
        "packagist/cssembed-bin": "~0.4.5",
        "verekia/initializr": "~4.2.0",
        "bluemesa/core-bundle": "dev-master"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "phpunit/phpunit": "~5.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
             "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
       ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.7-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "../core-bundle/"
        },
        {
            "type": "package",
            "package": {
                "name": "verekia/initializr",
                "version": "4.2.0",
                "dist":{
                    "url":"http://www.initializr.com/builder?modernizrrespond",
                    "type":"zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.11.3",
                "dist":{
                    "url":"http://code.jquery.com/jquery-1.11.3.min.js",
                    "type":"file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery-ui",
                "version": "1.11.4",
                "dist":{
                    "url":"http://jqueryui.com/resources/download/jquery-ui-1.11.4.zip",
                    "type":"zip",
                    "reference":"1.11.4"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "v3.0.2",
                "name": "twitter/hogan.js",
                "type": "component",
                "source": {
                    "url": "https://github.com/twitter/hogan.js.git",
                    "type": "git",
                    "reference": "v3.0.2"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "0.21",
                "name": "rejsmont/php-ipp",
                "source": {
                    "url": "https://github.com/rejsmont/php-ipp.git",
                    "type": "git",
                    "reference": "6c09140c99"
                },
                "autoload": {
                  "psr-0": { "PHP_IPP": ["lib/"] }
                }
            }
        }
    ]
}
