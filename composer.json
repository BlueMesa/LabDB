{
    "name": "bluemesa/labdb",
    "description": "Symfony2-based LIMS for Drosophila Labs",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "provide": {
        "components/bootstrap": "*",
        "components/jquery": "*"

    },
    "require": {
        "php": ">=5.6.0",
        "symfony/symfony": "~3.0",
        "doctrine/orm": "^2.4",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "symfony/assetic-bundle": "~2.7",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "twig/extensions": "~1.3",
        "jms/security-extra-bundle": "~1.5",
        "jms/di-extra-bundle": "~1.6",
        "jms/serializer-bundle": "~1.0",
        "jms/aop-bundle": "~1.2",
        "jms/parser-lib": "~1.0",
        "mopa/bootstrap-bundle": "dev-master#8a1bfa82398f339d9c53db24194626aed4fd4801",
        "knplabs/knp-paginator-bundle": "~2.5",
        "knplabs/knp-menu-bundle": "~2.1",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "friendsofsymfony/rest-bundle": "~2.0@dev",
        "whiteoctober/tcpdf-bundle": "~1.0.1",
        "sabre/vobject": "~3.4",
        "liuggio/ExcelBundle": "~2.0",
        "rejsmont/php-ipp": "~0.21",
        "kuleuven/shibboleth-bundle": "dev-master#67070be",
        "oyejorge/less.php": "~1.7",
        "leafo/scssphp": "~0.3",
        "packagist/yuicompressor-bin": "~2.4",
        "packagist/cssembed-bin": "~0.4",
        "verekia/initializr": "~4.2",
        "craue/formflow-bundle": "~3.0@dev",
        "beelab/bowerphp": "1.0.*@dev",
        "egulias/email-validator": "^1.2",
        "stof/doctrine-extensions-bundle": "^1.2"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "phpunit/phpunit": "~5.0",
        "symfony/phpunit-bridge": "~2.7.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "ln -fs ../../../../../../../../bower_components/bootstrap vendor/mopa/bootstrap-bundle/Mopa/Bundle/BootstrapBundle/Resources/public/bootstrap"
       ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "platform": {
            "php": "5.6.0"
        },
        "secure-http": false
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "0.21",
                "name": "rejsmont/php-ipp",
                "source": {
                    "url": "https://github.com/rejsmont/php-ipp.git",
                    "type": "git",
                    "reference": "6c09140c99"
                },
                "autoload": {
                  "psr-0": { "PHP_IPP": ["lib/"] }
                }
            }
        }
    ]
}
