{% extends "VIBFliesBundle:StockVial:list.html.twig" %}

{% block headline %}Select vials{% endblock headline %}

{% block table_attr %}id="select"{% endblock table_attr %}
{% block checkall_attr %}checked{%endblock checkall_attr %}

{% block list_selector %}
  {{ form_errors(form) }}
  {{ form_rest(form) }}
  <div class="form-inline control-group well">
    <input id="barcode" type="text" placeholder="Scan your barcodes here" value="" onchange="checkVial('stock')" />
    <span class="help-inline"></span>
  </div>
{% endblock list_selector %}
