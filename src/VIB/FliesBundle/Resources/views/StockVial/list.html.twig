{% extends "VIBFliesBundle:StockVial:layout.html.twig" %}
{% use "VIBBaseBundle:List:table.html.twig" %}
{% import "VIBBaseBundle:Macros:list.html.twig" as l %}
{% import 'VIBBaseBundle:Macros:status.html.twig' as status %}

{% if form is defined %}
  {% form_theme form _self %} 
  {% block _select_items_row %}{% endblock %}
{% endif %}

{% block headline %}Stock vials{% endblock headline %}

{% block list_selector %}
  {{ form_errors(form) }}
  {{ form_rest(form) }}
{% endblock list_selector %}

{% block list_header %}
  {% include 'VIBFliesBundle:StockVial:list_head.html.twig' with {'checked' : ''} %}
{% endblock list_header %}

{% block list_item %}
  {% include 'VIBFliesBundle:StockVial:list_row.html.twig' with {'entity': entity} %}
{% endblock list_item %}

{% block list_buttons %}
  {{ l.select_buttons() }}
{% endblock list_buttons %}
