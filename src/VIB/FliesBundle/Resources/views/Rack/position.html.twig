{% set uuid = random() ~ '_' ~ contents.id  %}

{% if order is not defined or order is empty %}
  {% set order = contents.id %}
{% endif %}

<span class="popover-trigger" id="span_{{ uuid }}"
      data-placement="top" data-container="#span_{{ uuid }}"
      data-delay="500" data-link="{{ path('vib_flies_ajax_popover') }}"
      data-type="vial" data-id="{{ contents.id }}" data-rack="{{ rackID }}">
  <a><input type="checkbox" id="select_items_{{ contents.id }}"
     name="select[items][{{ order }}]"
     value="{{ contents.id }}" checked />
  </a>
</span>
