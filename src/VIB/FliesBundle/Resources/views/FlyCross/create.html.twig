{% extends "VIBBaseBundle:Form:form.html.twig" %}

{% form_theme form _self %} 
  
{% block _FlyCrossType_vial_row %}
  {{ form_widget(form) }}
{% endblock %}

{% block modal_header %}New cross{% endblock modal_header %}

{% block modal_body %}
  {{ form_errors(form) }}
  {{ form_rest(form) }}
{% endblock modal_body %}

{% block modal_footer %}
  <button type="submit" class="btn btn-primary">Save</button>
  <button type="reset" class="btn">Reset</button>
  <a href="{{ path('flycross_list') }}" class="btn">Cancel</a>
{% endblock modal_footer %}
